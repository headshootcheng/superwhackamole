<%if(typeof errors !='undefined'){%>
<%errors.forEach(function(error){%> 
<div class="alert alert-warning alert-dismissible fade show" style ="background-color:red; color:white;"role="alert">
 <Strong><i class="material-icons">warning</i>  Warning:</strong>
  <%=error.msg%> 
  <button type="button" class="close" data-dismiss="alert" aria-label="Close">
    <span aria-hidden="true">&times;</span>
  </button>
</div>
<% }); %>
<%} %>

<%if(typeof forgoterrors !='undefined'){%>
  <%forgoterrors.forEach(function(error){%> 
  <div class="alert alert-warning alert-dismissible fade show" style ="background-color:red; color:white;"role="alert">
   <Strong><i class="material-icons">warning</i>  Warning:</strong>
    <%=error.msg%> 
    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
      <span aria-hidden="true">&times;</span>
    </button>
  </div>
  <% }); %>
  <%} %>

<% if (success_msg !=''){%>
<div class="alert alert-success alert-dismissible fade show" style ="background-color:green; color:black;"role="alert">
<Strong><i class="material-icons">assignment_turned_in</i></strong>
 <%= success_msg%>
  <button type="button" class="close" data-dismiss="alert" aria-label="Close">
    <span aria-hidden="true">&times;</span>
  </button>
</div>
<%}%>


<% if (error !=''){%>
<div class="alert alert-warning alert-dismissible fade show" style ="background-color:red; color:white; "role="alert" >
 <Strong><i class="material-icons">warning</i>  Warning:</strong>
  <%=error%> 
